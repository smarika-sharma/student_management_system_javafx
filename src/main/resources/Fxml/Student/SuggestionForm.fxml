               <font>
                  <Font name="Bell MT" size="45.0" />
               </font>
               <VBox.margin>
                  <Insets left="30.0" />
               </VBox.margin>
            </Label>
            <Label alignment="CENTER" prefHeight="30.0" prefWidth="741.0" text="Do you have any suggestions? Your suggestions help us better ">
               <font>
                  <Font size="20.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <Label alignment="CENTER" prefHeight="30.0" prefWidth="719.0" text="the platform. ">
               <font>
                  <Font size="20.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
      <TextField alignment="TOP_LEFT" layoutX="376.0" layoutY="271.0" prefHeight="311.0" prefWidth="732.0" promptText="Type your suggestion here...." style="-fx-border-color: #4D5194;">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Button layoutX="662.0" layoutY="635.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="118.0" styleClass="submit-button" stylesheets="@../../Style/SuggestionForm.css" text="Submit">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
