                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="SugessionForm" mnemonicParsing="false" onAction="#sugessionformclicked" prefHeight="65.0" prefWidth="234.0" styleClass="menu-button" text="Suggestion form">
               <font>
                  <Font size="16.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <VBox layoutX="351.0" layoutY="38.0" prefHeight="119.0" prefWidth="726.0">
