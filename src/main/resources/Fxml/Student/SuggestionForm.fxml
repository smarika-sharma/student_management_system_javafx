<AnchorPane prefHeight="750.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fx.studentmanagementsystem.controller.Teacher.TeacherDashboardController">
   <children>
      <VBox layoutX="44.0" layoutY="14.0" prefHeight="716.0" prefWidth="231.0" styleClass="menu" stylesheets="@../../Style/TeacherDashboard.css">
